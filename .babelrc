{
  "presets": [
    [
      "next/babel",
      {
        "preset-react": {
          "runtime": "automatic",
          "importSource": "@emotion/react"
        }
      }
    ]
  ],
  "plugins": [
    [
      "@emotion/babel-plugin",
      {
        "sourceMap": false,
        "autoLabel": "always",
        "labelFormat": "customPrefix-[filename]_[local]",
        "cssPropOptimization": true
      }
    ]
  ]
}
